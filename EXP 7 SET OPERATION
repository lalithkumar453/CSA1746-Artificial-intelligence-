# Define two sets
A = {1, 2, 3, 4, 5}
B = {4, 5, 6, 7, 8}

print("Set A:", A)
print("Set B:", B)

# 1. Union
print("Union of A and B:", A | B)       # or A.union(B)

# 2. Intersection
print("Intersection of A and B:", A & B)  # or A.intersection(B)

# 3. Difference
print("Difference (A - B):", A - B)     # or A.difference(B)
print("Difference (B - A):", B - A)     # or B.difference(A)

# 4. Symmetric Difference
print("Symmetric Difference:", A ^ B)   # or A.symmetric_difference(B)
